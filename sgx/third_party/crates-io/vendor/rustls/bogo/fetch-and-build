#!/bin/sh

set -e

# a known-good commit
COMMIT=8b60cde1ad8e25d99f9f83fdab9b3542a66217eb

rm -f runner.tar.gz
wget https://boringssl.googlesource.com/boringssl/+archive/$COMMIT/ssl/test/runner.tar.gz
mkdir -p bogo
cd bogo/
tar -xzf ../runner.tar.gz
patch -p1 < ../patches/testerrormap.diff
go test -c
