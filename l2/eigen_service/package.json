    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@ethersproject/shims": "^5.4.0",
    "asn1.js": "^5.4.1",
    "axios": "^0.23.0",
    "cors": "^2.8.5",
    "elliptic": "^6.5.4",
    "ethers": "^5.4.7",
    "express": "^4.17.1",
    "express-jwt": "^6.1.0",
    "googleapis": "^88.2.0",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.3.0",
    "secrets.js-grempe": "^2.0.0",
    "sequelize": "^6.7.0",
    "sqlite3": "^5.0.2",
