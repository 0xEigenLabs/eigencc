{
  "name": "eig-pkcs",
  "version": "1.0.0",
  "main": "build/src/app.js",
  "license": "Apache-2.0",
  "scripts": {
    "start": "yarn build && node ./build/src/app.js",
    "prebuild_close": "./node_modules/.bin/tslint -c tslint.json -p tsconfig.json --fix",
    "build": "./node_modules/.bin/tsc",
    "test": "yarn build && mocha ./build/**/*.test.js",
    "clean": "rm -f ./db_*.sqlite",
    "build_image": "docker build -t ieigen/service:v1 .",
    "deploy": "yarn build_image && docker run --name=eigen-service -p 3000:3000 -v $PWD/data:/app/data  -d ieigen/service:v1"
  },
  "devDependencies": {
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.10.3",
    "chai": "^4.3.4",
    "mocha": "^9.1.3",
